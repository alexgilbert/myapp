<h1>Listing Users</h1>

<%= link_to 'New User', new_user_path %>

<table>
   <tr>
      <th>Username</th>
      <th>Password</th>
      <th>Passowrd Confirm</th>
   </tr>

   <% @users.each do |user| %>
      <tr>
         <td><%= user.username %></td>
         <td><%= user.password.to_s.length - 3 %></td>
         <td> *** </td>
     </tr>
   <% end %>
</table>
